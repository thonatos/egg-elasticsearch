{
  "name": "egg-elasticsearch2",
  "version": "1.0.0",
  "description": "elasticsearch for egg",
  "eggPlugin": {
    "name": "elasticsearch2"
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "eggElasticsearch",
    "egg-elasticsearch"
  ],
  "dependencies": {
    "elasticsearch": "13.3.1"
  },
  "devDependencies": {
    "autod": "3.0.1",
    "autod-egg": "1.1.0",
    "egg": "1.13.1",
    "egg-bin": "4.6.3",
    "egg-ci": "1.8.0",
    "egg-mock": "3.17.0",
    "eslint": "3.19.0",
    "eslint-config-egg": "4.2.1",
    "supertest": "3.0.0",
    "webstorm-disable-index": "1.2.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
